{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "prisma": {
          "generate": true
        }
      }
    }
  ],
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "prisma generate && prisma migrate deploy && next build"
}